Cell[BoxData[{
 RowBox[{
  RowBox[{"numProcs", "=", 
   RowBox[{"{", 
    RowBox[{
    "2", ",", "2", ",", "4", ",", "4", ",", "8", ",", "8", ",", "8", ",", 
     "12", ",", "12", ",", "16", ",", "16", ",", "16", ",", " ", "32", ",", 
     " ", "64", ",", " ", "128"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"k", "=", "1"}], ",", 
   RowBox[{"k", "\[LessEqual]", 
    RowBox[{
     RowBox[{"Dimensions", "[", "numProcs", "]"}], "[", 
